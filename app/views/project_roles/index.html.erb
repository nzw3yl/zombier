<h1>Listing project_roles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Organization</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_roles.each do |project_role| %>
  <tr>
    <td><%= project_role.name %></td>
    <td><%= project_role.organization_id %></td>
    <td><%= project_role.active %></td>
    <td><%= link_to 'Show', project_role %></td>
    <td><%= link_to 'Edit', edit_project_role_path(project_role) %></td>
    <td><%= link_to 'Destroy', project_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

